program exercise1
  implicit none
  ! Schreiben Sie ein Programm, welche Variablen deklariert und mit einem beliebigen Wert initialisiert.
  ! Geben Sie diese Variablen mit entsprechenden Print-Anweisungen aus.
  integer :: i
  ! ...
  i = 42
  ! ...
  print *, "Exercise 1:"
  print *, "Variablen deklarieren, initialisieren und ausgeben."
  print *, "integer :: i = ", i
end program exercise1
